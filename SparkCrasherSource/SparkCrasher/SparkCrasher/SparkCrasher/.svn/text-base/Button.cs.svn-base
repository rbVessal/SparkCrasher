using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace SparkCrasher
{
    class Button
    {
        int x;
        int y;
        string thing;

        public int X
        {
            get { return x; }
            set { x = value; }
        }
        public int Y
        {
            get { return y; }
            set { y = value; }
        }
        public string Thing
        {
            get { return thing; }
            set { thing = value; }
        }
    }
}
